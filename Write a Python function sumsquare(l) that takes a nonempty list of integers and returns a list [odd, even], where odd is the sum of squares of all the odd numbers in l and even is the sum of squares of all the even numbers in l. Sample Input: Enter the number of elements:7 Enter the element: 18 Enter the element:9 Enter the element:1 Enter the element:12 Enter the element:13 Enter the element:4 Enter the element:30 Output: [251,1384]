def sumsquare(l):
    sum_odd = 0
    sum_even = 0
    for num in l:
        if num % 2 == 0:  # Check if the number is even
            sum_even += num ** 2
        else:             # Otherwise, it is odd
            sum_odd += num ** 2
    return [sum_odd, sum_even]
# Sample input:
l = [18, 9, 1, 12, 13, 4, 30]
print(sumsquare(l))  # Output: [251, 1384]
